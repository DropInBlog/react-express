{
  "name": "@dropinblog/react-express",
  "version": "1.0.0",
  "publishConfig": {
    "access": "public"
  },
  "repository": "https://github.com/DropInBlog/react-express",
  "description": "Express SSR middleware for @dropinblog/react-core",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs",
    "dev": "tsup src/index.ts --dts --format esm,cjs --watch",
    "typecheck": "tsc --noEmit"
  },
  "keywords": ["dropinblog", "react", "express", "ssr", "server-side-rendering"],
  "author": "DropInBlog",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "peerDependencies": {
    "react": "^18.2.0 || ^19.0.0-0",
    "react-dom": "^18.2.0 || ^19.0.0-0",
    "express": "^4.18.0 || ^5.0.0",
    "@dropinblog/react-core": "^1.0.6"
  },
  "devDependencies": {
    "@dropinblog/react-core": "^1.0.6",
    "@types/react": "^18.3.7",
    "@types/react-dom": "^18.3.1",
    "@types/express": "^4.17.21",
    "@types/node": "^24.10.1",
    "express": "^4.18.0",
    "react": "^19.0.0-rc-66855b96-20241106",
    "react-dom": "^19.0.0-rc-66855b96-20241106",
    "tsup": "^8.1.0",
    "typescript": "^5.5.4"
  }
}
